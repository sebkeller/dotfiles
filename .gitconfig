[init]
  defaultBranch = main
[color]
  ui = auto
[core]
  attributesfile = ~/.gitattributes
  excludesfile = ~/.gitignore
[commit]
  gpgsign = true
[diff]
  colorMoved = zebra
  colorMovedWS = allow-indentation-change
[fetch]
  prune = true
[pull]
  rebase = true
[push]
  default = current
[rebase]
  autosquash = true
  ff = only
